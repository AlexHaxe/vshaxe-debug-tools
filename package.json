{
    "name": "hxparservis",
    "publisher": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.4.0"
    },
    "activationEvents": [
        "onCommand:hxparservis.visualize"
    ],
    "main": "bin/hxparservis",
    "contributes": {
        "configuration": {
            "title": "hxparservis configuration",
            "properties": {
                "hxparservis.path": {
                    "description": "Path to the hxparser executable. If not defined, an embedded js version is used.",
                    "type": "string",
                    "default": null
                }
            }
        },
        "commands": [
            {
                "command": "hxparservis.visualize",
                "title": "hxparser: Visualize"
            }
        ]
    }
}